                <xs:enumeration value="ISO/IEC 80000" /> 
                <xs:enumeration value="si-digital-framework.org" /> 
                <xs:enumeration value="electropedia.org"/>
                <xs:enumeration value="mlayer.org" /> 
                <xs:enumeration value="qudt.org" /> 
