    <xs:element name="digitalCalibrationCertificate" type="dcx:digitalCalibrationCertificateType">
    <xs:key name="heading">
        <xs:selector xpath='dcx:statements/dcx:heading'/>
        <xs:field xpath="@lang"/>
    </xs:key>
    <xs:key name="PKequipment">
        <xs:selector xpath="dcx:equipment/dcx:equipmentItem"/>
        <xs:field xpath="@id"/>
    </xs:key>
    <xs:key name="PKsetting">
        <xs:selector xpath="dcx:settings/dcx:setting"/>
        <xs:field xpath="@settingId"/>
    </xs:key>
    <xs:key name="PKstatement">
        <xs:selector xpath="dcx:statements/dcx:statement"/>
        <xs:field xpath="@id"/>
    </xs:key>
    <xs:key name="PKmeasuringSystem">
        <xs:selector xpath="dcx:measuringSystems/dcx:measuringSystem"/>
        <xs:field xpath="@id"/>
    </xs:key>
    <xs:key name="PKmeasurementSeries">
        <xs:selector xpath="dcx:measurementResults/dcx:measurementSeries"/>
        <xs:field xpath="@tableId"/>
    </xs:key>

    <xs:keyref name="ms2equip" refer="dcx:PKequipment">
        <xs:selector xpath="dcx:measuringSystems/dcx:measuringSystem/dcx:equipmentRef"/>
        <xs:field xpath="@equipmentItemRef"/>
    </xs:keyref>
    <xs:keyref name="ms2statetment" refer="dcx:PKstatement">
        <xs:selector xpath="dcx:measuringSystems/dcx:measuringSystem/dcx:statementRef"/>
        <xs:field xpath="."/>
    </xs:keyref>
    <xs:keyref name="table2ms" refer="dcx:PKmeasuringSystem">
        <xs:selector xpath="dcx:measurementResults/dcx:calibrationResult"/>
        <xs:field xpath="@measuringSystemRef"/>
    </xs:keyref>
    <xs:keyref name="msequip2setting" refer="dcx:PKsetting">
        <xs:selector xpath="dcx:measuringSystems/dcx:measuringSystem/dcx:equipmentRef/dcx:settingRef"/>
        <xs:field xpath="."/>
    </xs:keyref>
    <xs:keyref name="exceptionRefColumn2statement" refer="dcx:PKstatement">
        <xs:selector xpath="dcx:measurementResults/dcx:calibrationResult/dcx:ataColumn/dcx:dataList/dcx:exceptionRef/dcx:row"/>
        <xs:field xpath="."/>
    </xs:keyref>
    <xs:keyref name="measurementSeriesRefColumn2measurementSeries" refer="dcx:PKmeasurementSeries">
        <xs:selector xpath="dcx:measurementResults/dcx:calibrationResult/dcx:column/dcx:dataList/dcx:measurementSeriesRef/dcx:row"/>
        <xs:field xpath="."/>
    </xs:keyref>
    </xs:element>

    <xs:complexType name="digitalCalibrationCertificateType">
        <xs:annotation>
            <xs:documentation>
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="heading" type="dcx:stringWithLangType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="administrativeData" type="dcx:administrativeDataType"/>
            <xs:element name="statements" type="dcx:statementListType" minOccurs="0"/>
            <xs:element name="equipment" type="dcx:equipmentListType"/>
            <xs:element name="settings" type="dcx:settingListType" minOccurs="0"/>
            <xs:element name="measuringSystems" type="dcx:measuringSystemListType"/>
            <xs:element name="measurementResults" type="dcx:measurementResultsType" maxOccurs="unbounded" minOccurs="0">
                <xs:unique name="calibrationResult">
                    <xs:selector xpath="dcx:calibrationResult"/>
                    <xs:field xpath="@measuringSystemRef"/>
                    <!--xs:field xpath="@serviceCategory"/>
                    <xs:field xpath="@customServiceCategory"/-->
                </xs:unique>
            </xs:element>
            <xs:element name="quantityUnitDefs" type="dcx:quantityUnitDefListType"/>
            <xs:element name="embeddedFiles" type="dcx:embeddedFileListType" maxOccurs="1" minOccurs="0"/>
            <xs:element name="comment" type="dcx:anyType" minOccurs="0"/>
            <!--xs:element ref="ds:Signature" minOccurs="0" maxOccurs="unbounded"/-->
        </xs:sequence>
        <xs:attribute name="schemaVersion" use="required">
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:pattern value="3\.0\.0"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
    </xs:complexType>

    <xs:complexType name="administrativeDataType">
        <xs:annotation>
            <xs:documentation>
                The element administrativeData contains all essential administrative information about the calibration.
                The entries in this area are basically the same and regulated in all DCCs.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="heading" type="dcx:stringWithLangType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="coreData" type="dcx:coreDataType"/>
            <xs:element name="dcxSoftware" type="dcx:softwareType"/> <!--complex-->
            <xs:element name="serviceProvider" type="dcx:contactType"/>
            <xs:element name="accreditation" type="dcx:accreditationType" minOccurs="0"/>
            <!--xs:element name="respPersons" type="dcx:respPersonListType"/-->
            <xs:element name="client" type="dcx:contactType"/> 
            <xs:element name="equipmentReturnInfo" type="dcx:contactType" minOccurs="0"/>
            <xs:element name="clientBillingInfo" type="dcx:contactType" minOccurs="0"/>
            <xs:element name="certificateReturnInfo" type="dcx:contactType" minOccurs="0"/>
            <xs:element name="performanceSite" type="dcx:contactType" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>
    
    <xs:complexType name="coreDataType">
        <xs:annotation>
            <xs:documentation>
                Important metadata for the DCC containing the global unique identifier and other identifications.
                Core data is a selection of simple fields. A heading in several languages may be connected to each field using the fieldHeadingType.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="fieldHeading" type="dcx:fieldHeadingType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="transactionContent" type="dcx:transactionContentType" /> <!--simple-->
            <xs:element name="uniqueIdentifier" type="xs:string"/> <!--simple-->
            <xs:element name="clientIdentifier" type="xs:string" minOccurs="0"/> <!--simple-->
            <xs:element name="devicesForMeasurement" type="xs:IDREFS" minOccurs="0"/> <!--simple-->
            <xs:element name="issueDate" type="xs:date"/> <!--simple-->
            <xs:element name="beginPerformanceDate" type="xs:date" minOccurs="0"/> <!--simple-->
            <xs:element name="endPerformanceDate" type="xs:date"/> <!--simple-->
            <xs:element name="equipmentReceiptDate" type="xs:date" minOccurs="0"/> <!--simple-->
            <xs:element name="equipmentReturnDate" type="xs:date" minOccurs="0"/> <!--simple-->
            <xs:element name="performanceLocation" type="dcx:performanceLocationType"/> <!--value with attribute -->
            <xs:element name="previousReport" type="dcx:hashType" minOccurs="0"/> <!--complex-->
            <xs:element name="countryCodeISO3166_1" type="dcx:stringISO3166Type"/> <!--simple-->
            <xs:element name="mandatoryLangCodeISO639_1" type="dcx:stringISO639Type"/> <!--simple-->
            <xs:element name="usedLangCodeISO639_1" type="dcx:stringISO639Type" minOccurs="0" maxOccurs="unbounded"/> <!--simple-->
            <xs:element name="conformityResult" type="dcx:conformityResultType" minOccurs="0"/> <!--value with attribute-->
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="columnHeadingType">
        <xs:annotation>
            <xs:documentation>
                A human readable heading of one of the columns in a sections.
                The attribute 'column' must refer to an element type of the section.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="heading" type="dcx:stringWithLangType" minOccurs="1" maxOccurs="unbounded"/>
        </xs:sequence>
        <xs:attribute name="column" type="xs:string" use="required"/>
    </xs:complexType>

    <xs:complexType name="fieldHeadingType">
        <xs:annotation>
            <xs:documentation>
                A human readable heading of one of the fields in a sections.
                The attribute 'field' must refer to an element type of the section.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="heading" type="dcx:stringWithLangType" minOccurs="1" maxOccurs="unbounded"/>
        </xs:sequence>
        <xs:attribute name="field" type="xs:string" use="required"/>
    </xs:complexType>

    <xs:complexType name="measuringSystemListType">
        <xs:annotation>
            <xs:documentation>
                A list of measuringSystem elements.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="heading" type="dcx:stringWithLangType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="columnHeading" type="dcx:columnHeadingType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="measuringSystem" type="dcx:measuringSystemType" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="measuringSystemType">
        <xs:annotation>
            <xs:documentation>
                set of one or more measuring instruments and often other
                components, assembled and adapted to give information used to
                generate measured values within specified intervals for
                quantities of specified kinds In the scope of
                DCC it is a list of references to elements of
                measuringInstrumentType and instrumentSettingType. Each table in
                the measutrementResults section must refer to exactely one
                element of measuringSystemType.
                Measuring system is a colection of simple fields. A number of measuring systems can be organized in a table with column headings defined by the measuringSystemListType.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="heading" type="dcx:stringWithLangType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="equipmentRefs" minOccurs="1" maxOccurs="unbounded">
                <xs:complexType>
                    <xs:simpleContent>
                        <xs:extension base='xs:IDREFS'>
                            <xs:attribute name="type" type="dcx:refTypeType" use="optional"/>
                        </xs:extension>
                    </xs:simpleContent>
                </xs:complexType>
            </xs:element>
            <xs:element name="settingRefs" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                    <xs:simpleContent>
                        <xs:extension base='xs:IDREFS'>
                            <xs:attribute name="type" type="dcx:refTypeType" use="optional"/>
                        </xs:extension>
                    </xs:simpleContent>
                </xs:complexType>
            </xs:element>
            <xs:element name="referenceRefs" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                    <xs:simpleContent>
                        <xs:extension base='xs:IDREFS'>
                            <xs:attribute name="type" type="dcx:refTypeType" use="optional"/>
                        </xs:extension>
                    </xs:simpleContent>
                </xs:complexType>
            </xs:element>
            <xs:element name="statementRefs" type="xs:IDREFS" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="operationalStatus" type="dcx:operationalStatusType" minOccurs="0" maxOccurs="1"/>
            <xs:element name="body" type="dcx:stringWithLangType" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
        <xs:attribute name="id" type="xs:ID" use="required"/>
    </xs:complexType>

    <xs:complexType name="equipmentRefType">
        <xs:sequence>
          <xs:element name="settingRef" type="xs:IDREF" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
        <xs:attribute name="equipmentItemRef" type="xs:IDREF" use="required"/>
    </xs:complexType>
	
	<xs:simpleType name="refTypeType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="statement"/>
			<xs:enumeration value="equipmentItem"/>
			<xs:enumeration value="setting"/>
			<xs:enumeration value="measuringSystem"/>
			<xs:enumeration value="hlink"/>
		</xs:restriction>
	</xs:simpleType>

    <!--xs:complexType name="refIdType">
        <xs:annotation>
            <xs:documentation>
                Reference to an element
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="heading" type="dcx:stringWithLangType" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
        <xs:attribute name="refId" type="xs:IDREFS" use="required"/>
    </xs:complexType-->

    <!--xs:complexType name="softwareListType">
        <xs:annotation>
            <xs:documentation>
                A list of software elements.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="heading" type="dcx:stringWithLangType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="columnHeading" type="dcx:columnHeadingType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="software" type="dcx:softwareType" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType-->


    <xs:complexType name="softwareType">
        <xs:annotation>
            <xs:documentation>
                Information about a software including its name, version and a description.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="heading" type="dcx:stringWithLangType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="name" type="dcx:notEmptyStringType"/>
            <xs:element name="release" type="dcx:notEmptyStringType"/>
            <xs:element name="type" minOccurs="0">
               <xs:simpleType>
                   <xs:restriction base="xs:string">
                       <xs:enumeration value="application" />
                       <xs:enumeration value="bios" />
                       <xs:enumeration value="driver" />
                       <xs:enumeration value="editor" />
                       <xs:enumeration value="firmware" />
                       <xs:enumeration value="library" />
                       <xs:enumeration value="os" />
                       <xs:enumeration value="other" />
                   </xs:restriction>
               </xs:simpleType>
           </xs:element>
        </xs:sequence>
        <xs:attribute name="id" type="xs:ID" use="optional"/>
    </xs:complexType>

    <xs:complexType name="conformityResultType">
        <xs:annotation>
            <xs:documentation>
                The conformityResultType is for the overall conformity of all devices measured in the report. Areference to a conformity statement explaining the criteria of the assessment is required.
            </xs:documentation>
        </xs:annotation>
        <xs:simpleContent>
            <xs:extension base="dcx:conformityStatusType">
                <xs:attribute name="conformityStatusRef" type="xs:IDREF" use="required"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>

    <xs:simpleType name="notEmptyStringType">
        <xs:restriction base="xs:string">
            <xs:pattern value="[^\s]+(\s+[^\s]+)*"/>
        </xs:restriction>
    </xs:simpleType>


    <xs:complexType name="equipmentListType">
        <xs:annotation>
            <xs:documentation>
                This element is a list of measuring instruments.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="heading" type="dcx:stringWithLangType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="columnHeading" type="dcx:columnHeadingType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="equipmentItem" type="dcx:equipmentItemType" minOccurs="1" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="equipmentItemType">
        <xs:annotation>
            <xs:documentation>
                Measuring instrument: I.e. equipment under test and reference equipment that has one or more identifiers (e.g. serial number).
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
        <xs:element name="heading" type="dcx:stringWithLangType" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element name="manufacturer" type="xs:string" minOccurs="0"/>
        <xs:element name="productName" type="xs:string" minOccurs="0"/>
        <xs:element name="productType" type="xs:string" minOccurs="0"/>
        <xs:element name="productNumber" type="xs:string" minOccurs="0"/>
        <xs:element name="clientId" type="xs:string" minOccurs="0"/>
        <xs:element name="manufacturerId" type="xs:string" minOccurs="0"/>
        <xs:element name="serviceProviderId" type="xs:string" minOccurs="0"/>
        <xs:element name="calDate" type="xs:date" minOccurs="0"/>
        <xs:element name="calDueDate" type="xs:date" minOccurs="0"/>
        <xs:element name="certId" type="xs:string" minOccurs="0"/> 
        <xs:element name="certProvider" type="xs:string" minOccurs="0"/> 
        </xs:sequence>
        <xs:attribute name="category"  type="dcx:equipmentCategoryType" use="required">
               <!-- <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="item"/>
                        <xs:enumeration value="reference"/>
                    </xs:restriction>
                </xs:simpleType>-->
        </xs:attribute>
        <xs:attribute name="id" type="xs:ID" use="required"/>
    </xs:complexType>

    <xs:simpleType name="equipmentCategoryType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="item"/> <!-- not used in VIM3 *TODO -->
			<xs:enumeration value="device"/> <!-- is used in VIM3 -->
			<xs:enumeration value="sensor"/> <!-- is used in VIM3 -->
			<xs:enumeration value="detector"/> <!-- is used in VIM3 -->
			<xs:enumeration value="transducer"/> <!-- -->
			<xs:enumeration value="transferDevice"/>
			<xs:enumeration value="reference"/> <!-- not used in VIM3-->
			<xs:enumeration value="referenceStandard"/> <!-- is used in VIM3, or Etalon -->
			<xs:enumeration value="referenceMaterial"/> <!-- is used in VIM3 -->
			<xs:enumeration value="model"/> <!-- is used in VIM3 2.48 -->
			<xs:enumeration value="environment"/> 
			<xs:enumeration value="other"/>
		</xs:restriction>
	</xs:simpleType>

    <!--xs:complexType name="identificationType">
        <xs:annotation>
            <xs:documentation>
                Identification: I.e. serial number or identification number.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
        <xs:element name="heading" type="dcx:stringWithLangType" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element name="value" type="xs:string" minOccurs="1"/>
        </xs:sequence>
        <xs:attribute name="issuer" type="dcx:issuerType">
        </xs:attribute>
    </xs:complexType-->

    <xs:simpleType name="operationalStatusType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="as found"/>
			<xs:enumeration value="after adjustment"/>
			<xs:enumeration value="after maintenance"/>
			<xs:enumeration value="after maintenance and adjustment"/>
		</xs:restriction>
	</xs:simpleType>

    <!--xs:simpleType name="issuerType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="client"/>
			<xs:enumeration value="manufacturer"/>
			<xs:enumeration value="serviceProvider"/>
		</xs:restriction>
	</xs:simpleType-->

    <xs:complexType name="performanceLocationType">
        <xs:annotation>
            <xs:documentation>
                This type contains information about the location where the the calibration is performed.
            </xs:documentation>
        </xs:annotation>
        <xs:simpleContent>
            <xs:extension base="dcx:stringPerformanceLocationType">
                <xs:attribute name="id" type="xs:ID" use="optional"/>
                <xs:attribute name="refId" type="xs:IDREFS" use="optional"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>

    <xs:simpleType name="stringPerformanceLocationType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="atProvider"/>
            <xs:enumeration value="inField"/>
            <!-- <xs:enumeration value="remote"/> -->
            <!-- <xs:enumeration value="client"/> -->
            <!-- <xs:enumeration value="laboratoryBranch"/> -->
            <!-- <xs:enumeration value="clientBranch"/> -->
            <!-- <xs:enumeration value="other"/> -->
        </xs:restriction>
    </xs:simpleType>


    <xs:simpleType name="transactionContentType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="digitalCalibrationCertificate"/>
            <xs:enumeration value="digitalTestReport"/>
            <xs:enumeration value="digitalReportOfSampling"/>
            <xs:enumeration value="digitalMeasurementReport"/>
            <xs:enumeration value="digitalExaminationCertificate"/>
            <xs:enumeration value="digitalConformityCertificate"/>
            
            <xs:enumeration value="request:DigitalCalibrationCertificate"/>
            <xs:enumeration value="request:digitalTestReport"/>
            <xs:enumeration value="request:digitalReportOfSampling"/>
            <xs:enumeration value="request:digitalMeasurementReport"/>
            <xs:enumeration value="request:digitalExaminationCertificate"/>
            <xs:enumeration value="request:digitalConformityCertificate"/>

            <xs:enumeration value="template:DigitalCalibrationCertificate"/>
            <xs:enumeration value="template:digitalMeasurementReport"/>
            <xs:enumeration value="template:digitalExaminationCertificate"/>
            <xs:enumeration value="template:digitalConformityCertificate"/>
            <xs:enumeration value="template:digitalTestReport"/>
        </xs:restriction>
    </xs:simpleType>
    
