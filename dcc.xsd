				<xs:enumeration value="Measure.Length.Form.Roughness" />
				<xs:enumeration value="Measure.Length.Form.Roundness" />
				<xs:enumeration value="Measure.Length.Form.Sphericity" />
				<xs:enumeration value="Measure.Length.Form.Straightness.Axis" />
				<xs:enumeration value="Measure.Length.Form.Straightness.Surface" />
				<xs:enumeration value="Measure.Length.Radius" />
				<xs:enumeration value="Measure.Mass.Apparent" />
				<xs:enumeration value="Measure.Mass.Conventional" />
				<xs:enumeration value="Measure.Mass.True" />
				<xs:enumeration value="Measure.Phase.PhaseModulation" />
				<xs:enumeration value="Measure.Phase.ReflectionFactor.RF" />
				<xs:enumeration value="Measure.Phase.TransmissionFactor" />
				<xs:enumeration value="Measure.PhaseNoise.SideBand" />
				<xs:enumeration value="Measure.Power.RF.Sinewave" />
				<xs:enumeration value="Measure.Pressure.Hydraulic.Static" />
				<xs:enumeration value="Measure.Pressure.Pneumatic.Absolute.Static" />
