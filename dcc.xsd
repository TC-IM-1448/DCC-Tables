    <xs:complexType name="measurementResultsType">
        <xs:annotation>
        <xs:documentation>
        Measurement results consists of a number of tables containing the calibration results with reference to relevant measuring systems under calibration, references used and applicalble statments.
        </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="heading" type="dcx:stringWithLangType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="body" type="dcx:stringWithLangType" minOccurs="0"/>
            <xs:element name="calibrationResult"  maxOccurs="unbounded" minOccurs="0">
                <xs:complexType>
                    <xs:complexContent>
                        <xs:extension base='dcx:tableType'>
                            <xs:attribute name="serviceCategory" type="dcx:serviceCategoryType" use="optional"/>
                        </xs:extension>
                    </xs:complexContent>
                </xs:complexType>
                <xs:unique name="column">
                    <xs:selector xpath="dcx:column"/>
                    <xs:field xpath="@dataCategoryRef"/>
                    <xs:field xpath="@scope"/>
                    <xs:field xpath="@quantity"/>
                    <xs:field xpath="@unit"/>
					<xs:field xpath="@quantityUnitDefRef"/>
                    <xs:field xpath="*/@dataCategory"/>
                </xs:unique>
            </xs:element>
            <xs:element name="dataFittingResult"  maxOccurs="unbounded" minOccurs="0">
                <xs:complexType>
                    <xs:complexContent>
                        <xs:extension base='dcx:tableType'>
                            <xs:attribute name="serviceCategory" type="dcx:serviceCategoryType" use="optional"/>
                        </xs:extension>
                    </xs:complexContent>
                </xs:complexType>
                <xs:unique name="dataFittingColumn">
                    <xs:selector xpath="dcx:column"/>
                    <xs:field xpath="@dataCategoryRef"/>
                    <xs:field xpath="@scope"/>
                    <xs:field xpath="@quantity"/>
                    <xs:field xpath="@unit"/>
					<xs:field xpath="@quantityUnitDefRef"/>
                    <xs:field xpath="*/@dataCategory"/>
                </xs:unique>
            </xs:element>
            <xs:element name="calibrationCurve"  maxOccurs="unbounded" minOccurs="0">
                <xs:complexType>
                    <xs:complexContent>
                        <xs:extension base='dcx:tableType'>
                            <xs:attribute name="serviceCategory" type="dcx:serviceCategoryType" use="optional"/>
                        </xs:extension>
                    </xs:complexContent>
                </xs:complexType>
                <xs:unique name="calibrationCurveColumn">
                    <xs:selector xpath="dcx:column"/>
                    <xs:field xpath="@dataCategoryRef"/>
                    <xs:field xpath="@scope"/>
                    <xs:field xpath="@quantity"/>
                    <xs:field xpath="@unit"/>
					<xs:field xpath="@quantityUnitDefRef"/>
                    <xs:field xpath="*/@dataCategory"/>
                </xs:unique>
            </xs:element>
            <xs:element name="measurementSeries" maxOccurs="unbounded" minOccurs="0">
                <xs:complexType>
                    <xs:complexContent>
                        <xs:extension base='dcx:tableType'>
                            <xs:attribute name="serviceCategory" type="dcx:serviceCategoryType" use="optional"/>
                        </xs:extension>
                    </xs:complexContent>
                </xs:complexType>
                <xs:unique name="measSerDataColumn">
                    <xs:selector xpath="dcx:column"/>
                    <xs:field xpath="@dataCategoryRef"/>
                    <xs:field xpath="@scope"/>
                    <xs:field xpath="@quantity"/>
                    <xs:field xpath="@unit"/>
                    <xs:field xpath="@quantityUnitDefRef"/>
                    <xs:field xpath="*/@dataCategory"/>
                </xs:unique>
            </xs:element>
        </xs:sequence>
    </xs:complexType>


    <xs:complexType name="columnType">
