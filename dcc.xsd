        </xs:sequence>
    </xs:complexType-->


    <xs:complexType name="settingListType">
        <xs:annotation>
            <xs:documentation>
                List of settings attached to a DCC.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="heading" type="dcx:stringWithLangType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="columnHeading" type="dcx:columnHeadingType" minOccurs="0" maxOccurs="unbounded"/>
            <!--xs:element name="body" type="dcx:stringWithLangType" minOccurs="0" maxOccurs="unbounded"/-->
            <xs:element name="setting" type="dcx:settingType" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="settingType">
        <xs:annotation>
            <xs:documentation>
                A setting is a selection of simple fields and attributes.
                A number of settings can be organized as rows in a table with columnHeadings defined by the settingListType.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <!--<xs:element name="countryCodeISO3166_1" type="dcx:stringISO3166Type" minOccurs="0"
                        maxOccurs="unbounded"/>-->
            <xs:element name="parameter" type="dcx:notEmptyStringType" minOccurs="0"/>
            <xs:element name="value" type="dcx:notEmptyStringType" minOccurs="0"/>
            <xs:element name="unit" type="dcx:notEmptyStringType" minOccurs="0"/>
            <xs:element name="softwareInstruction" type="dcx:notEmptyStringType" minOccurs="0"/>
            <xs:element name="heading" type="dcx:stringWithLangType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="body" type="dcx:stringWithLangType" minOccurs="0" maxOccurs="unbounded"/>	
            <xs:element name="statementRefs" type="xs:IDREFS" minOccurs="0"/>		
        </xs:sequence>
        <xs:attribute name="settingId" type="xs:ID" use="required"/>
        <xs:attribute name="equipmentRef" type="xs:IDREFS" use="required"/>
    </xs:complexType>

    <xs:complexType name="quantityUnitDefListType">
        <xs:annotation>
            <xs:documentation>
                List of definitions for quantities and units. 
                Can be used to introduce quantities and units which are not defined within this schema, and/or to specifiy the definition of a unit.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="heading" type="dcx:stringWithLangType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="body" type="dcx:stringWithLangType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="quantityUnitDef" type="dcx:quantityUnitDefType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="columnHeading" type="dcx:columnHeadingType" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="quantityUnitDefType">
        <xs:sequence>
            <xs:element name="quantityCode" type="dcx:notEmptyStringType" minOccurs="0"/>
