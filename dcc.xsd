    <xs:complexType name="columnType">
        <xs:annotation>
            <xs:documentation>
            Columns for measurement data and annotatin of data. Each column is ordered directly by the customer from the lists of allowed colTypes and measurandTypes.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="heading" type="dcc:stringWithLangType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="unit" type="dcc:unitType" minOccurs="0"/>
            <xs:element name="dataList" type="dcc:dataListType" minOccurs='0' maxOccurs="unbounded">
                <xs:unique name="idx0">
                    <xs:selector xpath="dcc:*"/>
                    <xs:field xpath="@idx"/>
                </xs:unique>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="scope" type="dcc:scopeType" use="required"/>
        <xs:attribute name="dataCategoryRef" type="dcc:dataCategoryType" use="required"/>
        <xs:attribute name="measurand" type="dcc:measurandType" use="required"/>
    </xs:complexType>
        
    <xs:complexType name="dataListType">
        <xs:choice>
            <xs:element name="value">
                <xs:complexType>
                    <xs:complexContent>
                        <xs:extension base='dcc:realDataType'>
                            <xs:attribute name="dataCategory" type="xs:string" fixed="value"/>
                        </xs:extension>
                    </xs:complexContent>
                </xs:complexType>
            </xs:element>
            <xs:element name="targetValue">
                <xs:complexType>
                    <xs:complexContent>
                        <xs:extension base='dcc:realDataType'>
                            <xs:attribute name="dataCategory" type="xs:string" fixed="targetValue"/>
                        </xs:extension>
                    </xs:complexContent>
                </xs:complexType>
            </xs:element>
            <xs:element name="expandedUncertainty">
                <xs:complexType>
                    <xs:complexContent>
                        <xs:extension base='dcc:realDataType'>
                            <xs:attribute name="dataCategory" type="xs:string" fixed="expandedUncertainty"/>
                        </xs:extension>
                    </xs:complexContent>
                </xs:complexType>
            </xs:element>
            <xs:element name="correlation">
                <xs:complexType>
                    <xs:complexContent>
                        <xs:extension base='dcc:realDataType'>
                            <xs:attribute name="dataCategory" type="xs:string" fixed="correlation"/>
                        </xs:extension>
                    </xs:complexContent>
                </xs:complexType>
            </xs:element>
            <xs:element name="correction">
                <xs:complexType>
                    <xs:complexContent>
                        <xs:extension base='dcc:realDataType'>
                            <xs:attribute name="dataCategory" type="xs:string" fixed="targetValue"/>
                        </xs:extension>
                    </xs:complexContent>
                </xs:complexType>
            </xs:element>
            <xs:element name="repeatability">
                <xs:complexType>
                    <xs:complexContent>
                        <xs:extension base='dcc:realDataType'>
                            <xs:attribute name="dataCategory" type="xs:string" fixed="repeatability"/>
                        </xs:extension>
                    </xs:complexContent>
                </xs:complexType>
            </xs:element>
            <xs:element name="conformity">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name='row' minOccurs='0' maxOccurs='unbounded'>
                            <xs:complexType>
                                <xs:simpleContent>
                                    <xs:extension base='dcc:conformityStatusType'>
                                        <xs:attribute name='idx' type='xs:int'/>
                                    </xs:extension>
                                </xs:simpleContent>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                    <xs:attribute name="dataCategory" type="xs:string" fixed="conformity"/>
                </xs:complexType>
            </xs:element>
            <xs:element name="rowTag" /> 
            <xs:element name="usedReferenceRef" />
            <xs:element name="methodRef" />
            <xs:element name="exceptionRef" type="dcc:exceptionRefType"/> 
            <xs:element name="traceabilityRef" /> 
            <xs:element name="attentionRef" /> 
            <xs:element name="commentRef" /> 
            <xs:element name="decisionRuleRef" /> 
            <xs:element name="accreditationExceptionRef" type="dcc:accreditationExceptionRefType"/> 
            <xs:element name="normRef" /> 
            <xs:element name="measurementSeriesRef" type="dcc:measurementSeriesRefType"/> 
            <xs:element name="uncertaintyCoverageFactor_k" type="dcc:uncertaintyCoverageFactor_kType"/>
            <xs:element name="uncertaintyCoverageProbability" />
