    <xs:simpleType name="quantityCodeSystemType">
        <xs:annotation>
            <xs:documentation>
            </xs:documentation>
        </xs:annotation>
            <xs:restriction base="xs:string">
                <xs:enumeration value="ISO/IEC 80000" /> 
                <xs:enumeration value="si-digital-framework.org" /> 
                <xs:enumeration value="electropedia.org"/>
                <xs:enumeration value="mlayer.org" /> 
                <xs:enumeration value="qudt.org" /> 
                <xs:enumeration value="ucum.org" /> 
                <xs:enumeration value="ptb.de" /> 
                <xs:enumeration value="other" /> 
            </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="calculateToSIunitExprType">
        <xs:annotation>
            <xs:documentation xml:lang="en">
				Based on xs:string.
                Should provide the mathematical expression to convert measurement values from the used unit to value in SI-unit 
                only allowed mathematical functions are: 
                    +, -, *, /, abs, exp, pwr, ln, log, sin, cos, tan, arcsin, arccos, arctan, 				
                    decimal numbers
                    value : identification of value in usedUnit
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="((([-+]?[0-9]+\.?[0-9]*)|\(|-|\+|\*|\/|abs\(|exp\(|pwr10\(|ln\(|\)|log\()*)value((([-+]?[0-9]+\.?[0-9]*)|\(|-|\+|\*|\/|abs\(|exp\(|pwr10\(|ln\(|\)|log10\()*)"/>
        </xs:restriction>
    </xs:simpleType>

