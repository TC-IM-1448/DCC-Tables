            MERCHANTABILITY or FITNESS FOR A PARTICULAR PURPOSE. See the
            GNU Lesser General Public License for more details.
        </xs:documentation>
    </xs:annotation>

    <xs:element name="digitalCalibrationCertificate" type="dcc:digitalCalibrationCertificateType">
    <xs:key name="heading">
        <xs:selector xpath='dcc:statements/dcc:heading'/>
        <xs:field xpath="@lang"/>
    </xs:key>
    <xs:key name="PKequipment">
        <xs:selector xpath="dcc:equipment/dcc:equipmentItem"/>
        <xs:field xpath="@id"/>
    </xs:key>
    <xs:key name="PKsetting">
        <xs:selector xpath="dcc:settings/dcc:setting"/>
        <xs:field xpath="@settingId"/>
    </xs:key>
    <xs:key name="PKstatement">
        <xs:selector xpath="dcc:statements/dcc:statement"/>
        <xs:field xpath="@id"/>
    </xs:key>
    <xs:key name="PKmeasuringSystem">
        <xs:selector xpath="dcc:measuringSystems/dcc:measuringSystem"/>
        <xs:field xpath="@id"/>
    </xs:key>
    <xs:key name="PKmeasurementSeries">
        <xs:selector xpath="dcc:measurementResults/dcc:measurementSeries"/>
        <xs:field xpath="@tableId"/>
    </xs:key>
    <xs:keyref name="ms2equip" refer="dcc:PKequipment">
        <xs:selector xpath="dcc:measuringSystems/dcc:measuringSystem/dcc:equipmentRef"/>
        <xs:field xpath="@equipmentItemRef"/>
    </xs:keyref>
    <xs:keyref name="ms2statetment" refer="dcc:PKstatement">
        <xs:selector xpath="dcc:measuringSystems/dcc:measuringSystem/dcc:statementRef"/>
        <xs:field xpath="."/>
    </xs:keyref>
    <xs:keyref name="table2ms" refer="dcc:PKmeasuringSystem">
        <xs:selector xpath="dcc:measurementResults/dcc:calibrationResult"/>
        <xs:field xpath="@measuringSystemRef"/>
    </xs:keyref>
    <xs:keyref name="msequip2setting" refer="dcc:PKsetting">
        <xs:selector xpath="dcc:measuringSystems/dcc:measuringSystem/dcc:equipmentRef/dcc:settingRef"/>
        <xs:field xpath="."/>
    </xs:keyref>
    <xs:keyref name="exceptionRefColumn2statement" refer="dcc:PKstatement">
        <xs:selector xpath="dcc:measurementResults/dcc:calibrationResult/dcc:ataColumn/dcc:dataList/dcc:exceptionRef/dcc:row"/>
        <xs:field xpath="."/>
    </xs:keyref>
    <xs:keyref name="measurementSeriesRefColumn2measurementSeries" refer="dcc:PKmeasurementSeries">
        <xs:selector xpath="dcc:measurementResults/dcc:calibrationResult/dcc:column/dcc:dataList/dcc:measurementSeriesRef/dcc:row"/>
        <xs:field xpath="."/>
    </xs:keyref>
    </xs:element>

    <xs:complexType name="digitalCalibrationCertificateType">
        <xs:annotation>
            <xs:documentation>
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>

            <xs:element name="heading" type="dcc:stringWithLangType" minOccurs="0" maxOccurs="unbounded"/>
            
            <xs:element name="administrativeData" type="dcc:administrativeDataType"/>
            <xs:element name="statements" type="dcc:statementListType" minOccurs="0"/>
            <xs:element name="equipment" type="dcc:equipmentListType"/>
            <xs:element name="settings" type="dcc:settingListType" minOccurs="0"/>
            <xs:element name="measuringSystems" type="dcc:measuringSystemListType"/>
            <xs:element name="measurementResults" type="dcc:measurementResultsType" maxOccurs="unbounded" minOccurs="0">
                <xs:unique name="calibrationResult">
                    <xs:selector xpath="dcc:calibrationResult"/>
                    <xs:field xpath="@measuringSystemRef"/>
                    <xs:field xpath="@serviceCategory"/>
                    <xs:field xpath="@customServiceCategory"/>
                </xs:unique>
            </xs:element>
            <xs:element name="embeddedFiles" type="dcc:embeddedFileListType" maxOccurs="1" minOccurs="0"/>

            <xs:element name="comment" minOccurs="0">
                <xs:complexType>
                    <xs:sequence>
                        <xs:any namespace="##any" processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>

            <!--xs:element ref="ds:Signature" minOccurs="0" maxOccurs="unbounded"/-->

        </xs:sequence>

        <xs:attribute name="schemaVersion" use="required">
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:pattern value="3\.0\.0"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
    </xs:complexType>

    <xs:complexType name="administrativeDataType">
        <xs:annotation>
            <xs:documentation>
                The element administrativeData contains all essential administrative information about the calibration.
                The entries in this area are basically the same and regulated in all DCCs.
            </xs:documentation>
        </xs:annotation>
        <xs:all>
            <xs:element name="dccSoftware" type="dcc:softwareListType"/>
            <xs:element name="coreData" type="dcc:coreDataType"/>
            <xs:element name="calibrationLaboratory" type="dcc:contactType"/>
            <xs:element name="accreditation" type="dcc:accreditationType" minOccurs="0"/>
            <xs:element name="respPersons" type="dcc:respPersonListType"/>
            <xs:element name="customer" type="dcc:contactType"/>
            <xs:element name="usedSoftware" type="dcc:softwareListType" minOccurs="0"/>
        </xs:all>
    </xs:complexType>

    <xs:complexType name="measuringSystemListType">
        <xs:annotation>
            <xs:documentation>
                A list of measuringSystem elements.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="heading" type="dcc:stringWithLangType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="measuringSystem" type="dcc:measuringSystemType" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="measuringSystemType">
        <xs:annotation>
            <xs:documentation>
                set of one or more measuring instruments and often other components, assembled and
adapted to give information used to generate measured values within specified intervals for
quantities of specified kinds
                In the scope of DCC it is a list of references to elements of measuringInstrumentType and instrumentSettingType. Each table in the measutrementResults section must refer to exactely one element of measuringSystemType.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="heading" type="dcc:stringWithLangType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="equipmentRefs" minOccurs="1" maxOccurs="unbounded">
                <xs:complexType>
                    <xs:simpleContent>
                        <xs:extension base='xs:IDREFS'>
                            <xs:attribute name="type" type="dcc:refTypeType" use="optional"/>
                        </xs:extension>
                    </xs:simpleContent>
                </xs:complexType>
			</xs:element>
            <xs:element name="settingRefs" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                    <xs:simpleContent>
                        <xs:extension base='xs:IDREFS'>
                            <xs:attribute name="type" type="dcc:refTypeType" use="optional"/>
                        </xs:extension>
                    </xs:simpleContent>
                </xs:complexType>            
			</xs:element>
            <xs:element name="statementRefs" type="xs:IDREFS" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="operationalStatus" type="dcc:operationalStatusType" minOccurs="0" maxOccurs="1"/>
            <xs:element name="body" type="dcc:stringWithLangType" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
        <xs:attribute name="id" type="xs:ID" use="required"/>
    </xs:complexType>

    <xs:complexType name="equipmentRefType">
        <xs:sequence>
          <xs:element name="settingRef" type="xs:IDREF" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
        <xs:attribute name="equipmentItemRef" type="xs:IDREF" use="required"/>
    </xs:complexType>
	
	<xs:simpleType name="refTypeType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="statement"/>
			<xs:enumeration value="equipmentItem"/>
			<xs:enumeration value="setting"/>
			<xs:enumeration value="measuringSystem"/>
			<xs:enumeration value="hlink"/>
		</xs:restriction>
	</xs:simpleType>

    <!--xs:complexType name="refIdType">
        <xs:annotation>
            <xs:documentation>
                Reference to an element
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="heading" type="dcc:stringWithLangType" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
        <xs:attribute name="refId" type="xs:IDREFS" use="required"/>
    </xs:complexType-->

    <xs:complexType name="softwareListType">
        <xs:annotation>
            <xs:documentation>
                A list of software elements.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="software" type="dcc:softwareType" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="softwareType">
        <xs:annotation>
            <xs:documentation>
                Information about a software including its name, version and a description.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="heading" type="dcc:stringWithLangType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="name" type="dcc:notEmptyStringType"/>
            <xs:element name="release" type="dcc:notEmptyStringType"/>
            <xs:element name="type" minOccurs="0">
               <xs:simpleType>
                   <xs:restriction base="xs:string">
                       <xs:enumeration value="application" />
                       <xs:enumeration value="bios" />
                       <xs:enumeration value="driver" />
                       <xs:enumeration value="editor" />
                       <xs:enumeration value="firmware" />
                       <xs:enumeration value="library" />
                       <xs:enumeration value="os" />
                       <xs:enumeration value="other" />
                   </xs:restriction>
               </xs:simpleType>
           </xs:element>
        </xs:sequence>
        <xs:attribute name="id" type="xs:ID" use="optional"/>
    </xs:complexType>


    <xs:complexType name="coreDataType">
        <xs:annotation>
            <xs:documentation>
                Important metadata for the DCC containing the global unique identifier and other identifications.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="countryCodeISO3166_1" type="dcc:stringISO3166Type"/>
            <xs:element name="usedLangCodeISO639_1" type="dcc:stringISO639Type" maxOccurs="unbounded"/>
            <xs:element name="mandatoryLangCodeISO639_1" type="dcc:stringISO639Type" maxOccurs="unbounded"/>
            <xs:element name="uniqueIdentifier" type="dcc:identificationType"/>
            <xs:element name="customerIdentification" type="dcc:identificationType" minOccurs="0"/>
            <xs:element name="receiptDate" type="xs:date" minOccurs="0"/>
            <xs:element name="beginPerformanceDate" type="xs:date"/>
            <xs:element name="endPerformanceDate" type="xs:date"/>
            <xs:element name="performanceLocation" type="dcc:performanceLocationType"/>
            <xs:element name="issueDate" type="xs:date" minOccurs="0"/>            
            <xs:element name="previousReport" type="dcc:hashType" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
    
    <xs:simpleType name="notEmptyStringType">
        <xs:restriction base="xs:string">
            <xs:pattern value="[^\s]+(\s+[^\s]+)*"/>
        </xs:restriction>
    </xs:simpleType>


    <xs:complexType name="equipmentListType">
        <xs:annotation>
            <xs:documentation>
                This element is a list of measuring instruments.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="heading" type="dcc:stringWithLangType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="equipmentItem" type="dcc:equipmentItemType" minOccurs="1" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="equipmentItemType">
        <xs:annotation>
            <xs:documentation>
                Measuring instrument: I.e. equipment under test and reference equipment that has one or more identifiers (e.g. serial number).
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
        <xs:element name="heading" type="dcc:stringWithLangType" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element name="manufacturer" type="xs:string" minOccurs="0"/>
        <xs:element name="productName" type="xs:string" minOccurs="0"/>
        <xs:element name="productType" type="xs:string" minOccurs="0"/>
        <xs:element name="productNumber" type="xs:string" minOccurs="0"/>
        <xs:element name="identification" type="dcc:identificationType" minOccurs="1" maxOccurs="unbounded"/>
        </xs:sequence>
        <xs:attribute name="category"  type="dcc:equipmentCategoryType" use="required">
               <!-- <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="item"/>
                        <xs:enumeration value="reference"/>
                    </xs:restriction>
                </xs:simpleType>-->
        </xs:attribute>
        <xs:attribute name="id" type="xs:ID" use="required"/>
    </xs:complexType>

    <xs:simpleType name="equipmentCategoryType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="item"/>
			<xs:enumeration value="indicating"/>
			<xs:enumeration value="transfering"/>
			<xs:enumeration value="sensing"/>
			<xs:enumeration value="reference"/>
			<xs:enumeration value="environment"/>
			<xs:enumeration value="other"/>
		</xs:restriction>
	</xs:simpleType>

    <xs:complexType name="identificationType">
        <xs:annotation>
            <xs:documentation>
                Identification: I.e. serial number or identification number.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
        <xs:element name="heading" type="dcc:stringWithLangType" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element name="value" type="xs:string" minOccurs="1"/>
        </xs:sequence>
        <xs:attribute name="issuer" type="dcc:issuerType">
        </xs:attribute>
    </xs:complexType>

    <xs:simpleType name="operationalStatusType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="as found"/>
			<xs:enumeration value="after adjustment"/>
			<xs:enumeration value="after maintenance"/>
			<xs:enumeration value="after maintenance and adjustme"/>
		</xs:restriction>
	</xs:simpleType>

    <xs:simpleType name="issuerType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="customer"/>
			<xs:enumeration value="manufacturer"/>
			<xs:enumeration value="calibrationLaboratory"/>
		</xs:restriction>
	</xs:simpleType>

    <xs:complexType name="performanceLocationType">
        <xs:annotation>
            <xs:documentation>
                This type contains information about the location where the the calibration is performed.
            </xs:documentation>
        </xs:annotation>
        <xs:simpleContent>
            <xs:extension base="dcc:stringPerformanceLocationType">
                <xs:attribute name="id" type="xs:ID" use="optional"/>
                <xs:attribute name="refId" type="xs:IDREFS" use="optional"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>

    <xs:simpleType name="stringPerformanceLocationType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="laboratory"/>
            <xs:enumeration value="customer"/>
            <xs:enumeration value="laboratoryBranch"/>
            <xs:enumeration value="customerBranch"/>
            <xs:enumeration value="other"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:complexType name="calibrationLaboratoryType">
        <xs:annotation>
            <xs:documentation>
                Information about the calibration laboratory.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="calibrationLaboratoryCode" type="dcc:notEmptyStringType" minOccurs="0"/>
            <xs:element name="contact" type="dcc:contactType"/>		
            <xs:element name="cryptElectronicSeal" type="xs:boolean" minOccurs="0"/>
            <xs:element name="cryptElectronicSignature" type="xs:boolean" minOccurs="0"/>
            <xs:element name="cryptElectronicTimeStamp" type="xs:boolean" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="respPersonListType">
        <xs:annotation>
            <xs:documentation>
                List of responsible persons for a DCC.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="respPerson" type="dcc:respPersonType" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="respPersonType">
        <xs:annotation>
            <xs:documentation>
                A person responsible for a DCC.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="heading" type="dcc:stringWithLangType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="name" type="dcc:notEmptyStringType"/>
            <xs:element name="mainSigner" type="xs:boolean" minOccurs="0"/>
            <xs:element name="cryptElectronicSeal" type="xs:boolean" minOccurs="0"/>
            <xs:element name="cryptElectronicSignature" type="xs:boolean" minOccurs="0"/>
            <xs:element name="cryptElectronicTimeStamp" type="xs:boolean" minOccurs="0"/>
        </xs:sequence>
        <xs:attribute name="id" type="xs:ID" use="optional"/>
    </xs:complexType>

    <xs:complexType name="settingListType">
        <xs:annotation>
            <xs:documentation>
                List of settings attached to a DCC.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="heading" type="dcc:stringWithLangType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="body" type="dcc:stringWithLangType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="setting" type="dcc:settingType" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="settingType">
        <xs:sequence>
            <!--<xs:element name="countryCodeISO3166_1" type="dcc:stringISO3166Type" minOccurs="0"
                        maxOccurs="unbounded"/>-->
            <xs:element name="parameter" type="dcc:notEmptyStringType" minOccurs="0"/>
            <xs:element name="value" type="dcc:notEmptyStringType" minOccurs="0"/>
            <xs:element name="unit" type="dcc:notEmptyStringType" minOccurs="0"/>
			<xs:element name="softwareInstruction" type="dcc:notEmptyStringType" minOccurs="0"/>
            <xs:element name="heading" type="dcc:stringWithLangType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="body" type="dcc:stringWithLangType" minOccurs="0" maxOccurs="unbounded"/>		</xs:sequence>
        <xs:attribute name="settingId" type="xs:ID" use="required"/>
        <xs:attribute name="equipmentRef" type="xs:IDREFS" use="required"/>
    </xs:complexType>

    <xs:complexType name="statementListType">
        <xs:annotation>
            <xs:documentation>
                List of statements attached to a DCC.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="heading" type="dcc:stringWithLangType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="body" type="dcc:stringWithLangType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="statement" type="dcc:statementType" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="statementType">
        <xs:sequence>
            <xs:element name="heading" type="dcc:stringWithLangType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="body" type="dcc:stringWithLangType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="externalReference" type="dcc:notEmptyStringType" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
        <xs:attribute name="id" type="xs:ID" use="required"/>
        <xs:attribute name="category" type="dcc:statementCategoryType" use="required"/>
    </xs:complexType>

    <xs:simpleType name="statementCategoryType">
        <xs:annotation>
            <xs:documentation>
            </xs:documentation>
        </xs:annotation>
            <xs:restriction base="xs:string">
                <xs:enumeration value="method" /> 
                <xs:enumeration value="general" /> 
                <xs:enumeration value="exception" /> 
                <xs:enumeration value="traceability" /> 
                <xs:enumeration value="attention" /> 
                <xs:enumeration value="comment" /> 
                <xs:enumeration value="accreditation" /> 
                <xs:enumeration value="accreditationException" /> 
                <xs:enumeration value="decisionRule" /> 
                <xs:enumeration value="norm" /> 
            </xs:restriction>
    </xs:simpleType>
	
	<xs:simpleType name="tableCategoryType">
        <xs:annotation>
            <xs:documentation>
            </xs:documentation>
        </xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="calibrationResult" /> 
			<xs:enumeration value="measurementSeries" /> 
		</xs:restriction>
    </xs:simpleType>

    <xs:complexType name="tableType">
        <xs:annotation>
            <xs:documentation>
                This type is for arranging any measurement data into a table with rows and columns. It is also allowed to include images and plots in the form of references to embedded files.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="heading" type="dcc:stringWithLangType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="body" type="dcc:stringWithLangType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="column" type="dcc:columnType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="embeddedFileRef" type="xs:IDREF" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
		<xs:attribute name="tableId" type="xs:ID" use="required"/>
		<xs:attribute name="measuringSystemRef" type="xs:IDREF" use="required"/>
		<xs:attribute name="numRows" type="xs:int" use="required"/>
		<xs:attribute name="numCols" type="xs:int" use="required"/>
		<xs:attribute name="customServiceCategory" type="xs:string" use="optional" default="BIPM"/>
		<xs:attribute name="statementRef" type="xs:IDREFS" use="optional"/>		
    </xs:complexType>

    <xs:complexType name="measurementResultsType">
        <xs:annotation>
        <xs:documentation>
        Measurement results consists of a number of tables containing the calibration results with reference to relevant measuring systems under calibration, references used and applicalble statments.
        </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="heading" type="dcc:stringWithLangType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="body" type="dcc:stringWithLangType" minOccurs="0"/>
            <xs:element name="calibrationResult"  maxOccurs="unbounded" minOccurs="0">
                <xs:complexType>
                    <xs:complexContent>
                        <xs:extension base='dcc:tableType'>
                            <xs:attribute name="serviceCategory" type="dcc:serviceCategoryType" use="required"/>
                        </xs:extension>
                    </xs:complexContent>
                </xs:complexType>
                <xs:unique name="column">
                    <xs:selector xpath="dcc:column"/>
                    <xs:field xpath="@dataCategoryRef"/>
                    <xs:field xpath="@scope"/>
                    <xs:field xpath="@measurand"/>
                    <xs:field xpath="dcc:unit"/>
                    <xs:field xpath="dcc:dataList/*/@dataCategory"/>
                </xs:unique>
            </xs:element>
            <xs:element name="measurementSeries" maxOccurs="unbounded" minOccurs="0">
                <xs:complexType>
                    <xs:complexContent>
                        <xs:extension base='dcc:tableType'>
                            <xs:attribute name="serviceCategory" type="dcc:serviceCategoryType" use="optional"/>
                        </xs:extension>
                    </xs:complexContent>
                </xs:complexType>
                <xs:unique name="measSerDataColumn">
                    <xs:selector xpath="dcc:column"/>
                    <xs:field xpath="@dataCategoryRef"/>
                    <xs:field xpath="@scope"/>
                    <xs:field xpath="@measurand"/>
                    <xs:field xpath="dcc:unit"/>
                    <xs:field xpath="dcc:dataList/*/@dataCategory"/>
                </xs:unique>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="CMCListType">
        <xs:annotation>
            <xs:documentation>
                List of approved Calibration and Measurement Capabilities (CMC) applicable to results in this DCC.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="heading" type="dcc:stringWithLangType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="CMC" type="dcc:accreditationType" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="accreditationType">
        <xs:annotation>
            <xs:documentation>
                Approved Calibration and Measurement Capability (CMC) applicable to results in this DCC.
                Mandatotry fields: 
                Name and country of the accreditation body
                Id of the calibration laboratory in the system of the accreitation body
                Applicability of accreditation for results in this DCC: 'wholly' (applies to all results) or 'partial' (exceptions occur)

            </xs:documentation>
        </xs:annotation>
		<xs:sequence>
			<xs:element name="heading" type="dcc:stringWithLangType" minOccurs="1" maxOccurs="unbounded"/>
            <xs:element name="accreditationCountry" minOccurs="1"/>
            <xs:element name="accreditationBody" minOccurs="1"/>
            <xs:element name="accreditationLabId" minOccurs="1"/>
            <xs:element name="accreditationNorm" minOccurs="0"/> <!--type with specific values i.e. 17025, 17035, ... -->
            <xs:element name="accreditationApplicability" minOccurs="1" type="dcc:accreditationApplicabilityType"/>
        </xs:sequence>
        <!-- <xs:attribute name="accrId" type="xs:ID" use="required"/> --> 
    </xs:complexType>

    <xs:simpleType name="accreditationApplicabilityType">
        <xs:annotation>
            <xs:documentation>
             "partial: results outside accreditation occurs and these are clearly marked. Use column with attribute metaDataCategory=accreditationException to indicate this
              wholly: all results are under accreditation."
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="partial"/>
            <xs:enumeration value="wholly"/>
        </xs:restriction>
    </xs:simpleType>


